{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm ci --prefer-offline --no-audit --no-fund --legacy-peer-deps",
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, immutable, max-age=31536000"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "build": {
    "env": {
      "NODE_VERSION": "20"
    }
  }
}